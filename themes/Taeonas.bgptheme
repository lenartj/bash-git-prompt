override_git_prompt_colors() {
  GIT_PROMPT_THEME_NAME="Taeonas"

  GIT_PROMPT_PREFIX="("
  GIT_PROMPT_SUFFIX=")"
  GIT_PROMPT_SEPARATOR=""
  GIT_PROMPT_STAGED=" ${Red}●${ResetColor}"
  GIT_PROMPT_CONFLICTS=" ${Red}✖${ResetColor}"
  GIT_PROMPT_CHANGED=" ${Blue}✚${ResetColor}"
  GIT_PROMPT_UNTRACKED=" ${Cyan}…${ResetColor}"
  GIT_PROMPT_STASHED=" ${BoldBlue}⚑${ResetColor}"
  GIT_PROMPT_CLEAN=" ${BoldGreen}✔${ResetColor}"

  GIT_PROMPT_COMMAND_OK=""
  GIT_PROMPT_COMMAND_FAIL=" ${Red}_LAST_COMMAND_STATE_${ResetColor} "

  GIT_PROMPT_START_USER="\u@\h:${PathShort}"
  GIT_PROMPT_END_USER="${ResetColor}_LAST_COMMAND_INDICATOR_$ "
  GIT_PROMPT_END_ROOT="${ResetColor}_LAST_COMMAND_INDICATOR_# "

}

reload_git_prompt_colors "Taeonas"
